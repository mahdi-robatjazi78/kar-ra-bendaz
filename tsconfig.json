{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module":"commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    // "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "sourceMap": true,
    "baseUrl":".",
    "paths": {
      "@/*":["./src/*"],
      "@utils/*" :["./src/util/*"],
      "@services/*" :["./src/services/*"],
      "@context/*" : ["./src/context/*"],
      "@compo/*" : ["./src/components/*"],
      "@hooks/*" : ["./src/hooks/*"]
    } 
  },
    "include": [
    "src",
    "src/components",
    "src/custom.d.ts"
  ],
  
}
